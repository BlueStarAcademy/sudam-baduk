{
  "name": "gemini-go-app",
  "version": "1.0.0",
  "description": "Go game with Gemini and KataGo AI",
  "type": "module",
  "main": "server/server.ts",
  "scripts": {
    "start": "concurrently \"npm:start-client\" \"npm:start-server\"",
    "start-client": "vite --host",
    "start-server": "npx tsx watch --tsconfig server/tsconfig.json server/server.ts",
    "build": "vite build"
  },
  "dependencies": {
    "@google/genai": "^1.13.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.39",
    "tailwindcss": "3.4.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.0"
  }
}